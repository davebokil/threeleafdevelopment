<!--================= main ====================-->
<div id="main">
    <!--=============== header ================-->
    {{> header}}
    <!--=============== wrapper ===============-->
    <div id="wrapper">
        <!-- content-holder  -->
        <div class="content-holder">
            <!-- Page title -->
            <div class="dynamic-title">{{title}}</div>
            <!-- Page title  end-->
            <!-- content  -->
            <div class="content">
                <section class="parallax-section">
                    <div class="parallax-inner">
                        <div class="bg" data-bg="{{header}}" data-top-bottom="transform: translateY(300px);" data-bottom-top="transform: translateY(-300px);"></div>
                        <div class="overlay"></div>
                    </div>
                    <div class="container">
                        <div class="page-title">
                            <div class="row">
                                <div class="col-md-6">
                                    <h2><strong> Leasings</strong></h2>
                                </div>
                                <div class="col-md-6">
                                    <ul class="creat-list">
                                        <li><a href="/">Home</a></li>
                                        <li><a href="/leasings">Leasings</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <!-- content end -->
            <!-- content  -->
            <div class="content">
                <section>
                    <div class="container">
                        <div class="services-holder">
                            <!-- Leasings -->
                            {{#if leasings}}
                                {{#each leasings}}
                                <div class="row">
                                    <div class="col-md-7 col-md-offset-2">
                                        <div class="parallax-item left-direction">
                                            <div class="paralax-media">
                                                <div class="paralax-wrap">
                                                    <img src="{{thumbnail}}?q=&auto=compress,enhance," class="respimg" alt="{{title}}">
                                                </div>
                                            </div>
                                            <div class="parallax-deck" data-top-bottom="transform: translateY(-200px);" data-bottom-top="transform: translateY(200px);">
                                                <div class="parallax-deck-item">
                                                    <h3><strong>{{title}}</strong></h3>
                                                    <a href="/leasings/{{slug}}" class="btn anim-button fl-l"><span>View Leasings</span><i class="fa fa-long-arrow-right"></i></a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-5"></div>
                                </div>
                                {{/each}}
                            {{/if}}
                            <!-- Leasings end -->
                        </div>
                    </div>
                </section>
            </div>
            <!-- content end -->
            {{> footer}}
            <!-- share  -->
            <div class="share-inner">
                <div class="share-container  isShare" data-share="['facebook','googleplus','twitter','linkedin']"></div>
                <div class="close-share"></div>
            </div>
            <!-- share end -->
        </div>
        <!-- content-holder  end-->
    </div>
    <!--=============== footer ================-->
    {{> leftbar}}
</div>